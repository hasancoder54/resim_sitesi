{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload.js",
      "methods": ["POST"]
    },
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}